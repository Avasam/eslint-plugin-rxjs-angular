{
  "name": "eslint-plugin-rxjs-angular-x",
  "type": "commonjs",
  "version": "0.0.0",
  "packageManager": "yarn@4.6.0+sha512.5383cc12567a95f1d668fbe762dfe0075c595b4bfff433be478dbbe24e05251a8e8c3eb992a986667c1d53b6c3a9c85b8398c35a960587fbd9fa3a0915406728",
  "description": "ESLint v9+ rules for RxJS and Angular",
  "author": "Jason Weinzierl <weinzierljason@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/JasonWeinzierl/eslint-plugin-rxjs-angular-x",
  "repository": {
    "type": "git",
    "url": "https://github.com/JasonWeinzierl/eslint-plugin-rxjs-angular-x.git"
  },
  "bugs": {
    "url": "https://github.com/JasonWeinzierl/eslint-plugin-rxjs-angular-x/issues"
  },
  "keywords": [
    "lint",
    "rules",
    "eslint",
    "rxjs",
    "angular"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dist": "yarn run lint && yarn run dist:build",
    "dist:build": "yarn run dist:clean && tsc -p tsconfig-dist.json",
    "dist:clean": "rimraf dist",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "prepublishOnly": "yarn run test && yarn run dist",
    "prettier": "prettier --write \"./{src,tests}/**/*.{js,json,ts,tsx}\"",
    "prettier:ci": "prettier --check \"./{src,tests}/**/*.{js,json,ts,tsx}\"",
    "test": "yarn run test:rules",
    "test:debug": "mocha -r ts-node/register -t 5000 tests/rules/prefer-async-pipe.test.ts",
    "test:rules": "mocha -r ts-node/register -t 5000 tests/rules/*.test.ts"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.0.0",
    "common-tags": "^1.8.0",
    "eslint-etc": "^5.0.0",
    "requireindex": "~1.2.0",
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "typescript": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@cartant/eslint-config": "^3.0.0",
    "@types/chai": "^4.2.0",
    "@types/common-tags": "^1.8.0",
    "@types/eslint": "^8.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.2.0",
    "eslint": "^8.0.0",
    "lint-staged": "^13.0.0",
    "mocha": "^9.0.0",
    "prettier": "^2.0.0",
    "rimraf": "^3.0.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.7.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
